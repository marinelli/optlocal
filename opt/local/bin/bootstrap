#!/bin/sh

CMD_NAME="${0##*/}"

EXIT_SUCCESS=0
EXIT_FAILURE=1

COMMON_SH_PATH='/opt/local/lib/common.sh'

if [ ! -f "$COMMON_SH_PATH" ] ; then
  printf '!! %s : %s\n' "$CMD_NAME" "${COMMON_SH_PATH##*/} is missing"
  exit $EXIT_FAILURE
fi

if . "$COMMON_SH_PATH" ; then
  load_all_libs
else
  printf '!! %s : %s\n' "$CMD_NAME" 'something gone wrong'
  exit $EXIT_FAILURE
fi

bootstrap $@

